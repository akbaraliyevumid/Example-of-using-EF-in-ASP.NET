@model IEnumerable<RegistrationFormASP.Models.User>
@{
    ViewData["Title"] = "Home Page";
}

<a asp-action="Create">Adding user</a>
<table class="table">
    <tr>
        <td>
            Name
        </td>
        <td>
            Surname
        </td>
        <td>
            Age
        </td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Surname
            </td>
            <td>
                @item.Age
            </td>
        </tr>
    }
</table>